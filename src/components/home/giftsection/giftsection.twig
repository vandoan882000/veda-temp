
{% set uniqueId = "id_2ff4a9a1-d701-40b9-9ca6-9bc63e755318" %}
{% set backgroundFixed = general_settings.fixed ? 'fixed' : 'scroll' %}
{% set backgroundColor = general_settings.background_color_enable ? 'background-color:' ~  general_settings.color ~  ';' : '' %}
{% set backgroundImage = general_settings.background_image_enable ? 'background-image: url(' ~  general_settings.image ~  ');background-attachment:' ~  backgroundFixed ~  ';background-size:' ~  general_settings.size ~  ';' : '' %}

<section data-id="giftsection" class="p:0px_60px@md" css="{{ backgroundColor }}{{ backgroundImage }}">
  <div class="veda-overlay" css="background-color: {{ general_settings.overlay }}"></div>
  <div class="container">
    <div class="row mt:0px">
      {% for item in content %}

        <div class="col-xs-{{column.xs}} col-sm-{{column.sm}} col-md-{{column.md}} col-lg-{{column.lg}} mt:0px">
          <div class="yasmina-gift d:flex h:84px jc:center ai:center fw:500" component="gift">
            <a class="cur:pointer w:100% c:color-dark fz:17px fw:500" href="#">
              <i class="{{item.icon}} pr:15px fz:23px fw:300"></i><span>{{item.label}}</span>
            </a>
          </div>
        </div>
      

{% endfor %}


    </div>
  </div>
</section>
